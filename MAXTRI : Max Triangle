#include <bits/stdc++.h>
using namespace std;

int main() {
    // your code goes here
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        // Bas issi case mein answer possible nahi hoga.
        if (n == 3) {
            cout << -1 << endl;
            continue;
        }
        // Har case ka maximum possible perimeter.
        int ans = n + n - 1 + n - 2;
        cout << ans << endl;
    }
}
